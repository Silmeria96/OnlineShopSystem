@using OnlineShopSystem.UI.Util

@{
    string websiteName = CommonFunc.GetConfig("WebsiteName");
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - @websiteName</title>
    @Styles.Render("~/Content/Shared/css")
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/swiper.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Scripts/vue.min.js"></script>
    <scripts src="~/Scripts/axios.min.js"></scripts>
</head>
<body>

    @Html.Partial("_Layout_NavPartial")
    
    @RenderBody()

    @Html.Partial("_Layout_FooterPartial")

    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("scripts", required: false)

</body>
</html>
